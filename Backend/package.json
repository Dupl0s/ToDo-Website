{
  "name": "ToDo-Backend",
  "version": "1.0.50",
  "scripts": {
    "test": "vitest --ui",
    "test:once": "vitest run",
    "dev": "nodemon --watch src --ext ts --exec \"tsc && node dist/index.js\"",
    "build": "tsc",
    "start": "ts-node src/index.ts",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src",
    "format": "prettier --write src/**/*.ts"
  },
  "dependencies": {
    "express": "latest"
  },
  "devDependencies": {
    "@types/express": "latest",
    "@types/node": "latest",
    "@vitest/ui": "latest",
    "eslint": "latest",
    "eslint-config-prettier": "latest",
    "nodemon": "latest",
    "prettier": "latest",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "latest",
    "vitest": "latest"
  }
}
